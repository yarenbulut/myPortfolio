  "version": 2,
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "regions": ["fra1"],
  "alias": ["yarenbulut.com", "www.yarenbulut.com"]
} 